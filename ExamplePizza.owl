<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.pizza/odontologies/pizza.owl"
     xml:base="http://www.pizza/odontologies/pizza.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:pizza="http://www.pizza/odontologies/pizza.owl#">
    <owl:Ontology rdf:about="http://www.pizza/odontologies/pizza.owl">
        <rdfs:comment xml:lang="es">La ontología de la pizza que describe varias pizzas basadas en sus ingredientes.</rdfs:comment>
        <rdfs:comment>The pizza ontology that describes various pizzas based on their toppings</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza/odontologies/pizza.owl#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.pizza/odontologies/pizza.owl#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza/odontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza/odontologies/pizza.owl#isBaseOf"/>
        <rdfs:domain rdf:resource="http://www.pizza/odontologies/pizza.owl#Pizza"/>
        <rdfs:range rdf:resource="http://www.pizza/odontologies/pizza.owl#PizzaBase"/>
        <rdfs:comment xml:lang="es">Tiene una base</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza/odontologies/pizza.owl#hasCountryOfOrigin -->

    <owl:ObjectProperty rdf:about="http://www.pizza/odontologies/pizza.owl#hasCountryOfOrigin">
        <rdfs:comment xml:lang="es">Representa el país de origen</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza/odontologies/pizza.owl#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.pizza/odontologies/pizza.owl#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.pizza/odontologies/pizza.owl#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="es">Tiene ingredientes</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza/odontologies/pizza.owl#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.pizza/odontologies/pizza.owl#hasSpiciness">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.pizza/odontologies/pizza.owl#SpicinessValuePartition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza/odontologies/pizza.owl#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.pizza/odontologies/pizza.owl#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza/odontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza/odontologies/pizza.owl#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.pizza/odontologies/pizza.owl#Pizza"/>
        <rdfs:range rdf:resource="http://www.pizza/odontologies/pizza.owl#PizzaTopping"/>
        <rdfs:comment xml:lang="es">Tiene un adicional</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza/odontologies/pizza.owl#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza/odontologies/pizza.owl#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza/odontologies/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.pizza/odontologies/pizza.owl#PizzaBase"/>
        <rdfs:range rdf:resource="http://www.pizza/odontologies/pizza.owl#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza/odontologies/pizza.owl#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza/odontologies/pizza.owl#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="es">Inverso de tiene. Para decir es un ingrediente de.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza/odontologies/pizza.owl#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza/odontologies/pizza.owl#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza/odontologies/pizza.owl#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza/odontologies/pizza.owl#hasCalorificContentValue -->

    <owl:DatatypeProperty rdf:about="http://www.pizza/odontologies/pizza.owl#hasCalorificContentValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Defined value calorific</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.pizza/odontologies/pizza.owl#hasCookingTime -->

    <owl:DatatypeProperty rdf:about="http://www.pizza/odontologies/pizza.owl#hasCookingTime">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Tiempo de cocción</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.pizza/odontologies/pizza.owl#hasToBake -->

    <owl:DatatypeProperty rdf:about="http://www.pizza/odontologies/pizza.owl#hasToBake">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza/odontologies/pizza.owl#AmericanaHotPizza -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#AmericanaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#JalopenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#JalopenoPepperTopping"/>
                            <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A pizza that only has Mozarella and Tomato toppings</rdfs:comment>
        <rdfs:comment xml:lang="es">Una pizza que solo tiene Mozarella y Coberturas de tomate</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#AmericanaPizza -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A pizza that only has Mozarella and Tomato toppings</rdfs:comment>
        <rdfs:comment xml:lang="es">Una pizza que solo tiene Mozarella y Coberturas de tomate</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#AnchovyTopping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#SeafoodTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#BasilTopping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#BasilTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#BlackRaspberryTopping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#BlackRaspberryTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#RaspberryTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza/odontologies/pizza.owl#RedRaspberryTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#CaperTopping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#CheeseTopping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#CheesyPizza -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#Country -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#Country">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#America"/>
                    <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#England"/>
                    <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#France"/>
                    <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#Germany"/>
                    <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#Italy"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#DeepPanBase -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.pizza/odontologies/pizza.owl#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#FourCheesePizza -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#FourCheesePizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.pizza/odontologies/pizza.owl#CheeseTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#FourSeasonsPizza -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#FourSeasonsPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasIngredient"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#AnchovyTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasIngredient"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#BasilTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasIngredient"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasIngredient"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#MushroomsTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasIngredient"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#RedPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasIngredient"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#SalamiTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasIngredient"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="es">Pizza 4 estaciones su ingrediente es la anchoa, campiñones y pimientos</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#FruitsTopping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#FruitsTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#GarlicTopping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#GarlicTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#PepperTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#HamTopping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#HighCaloriePizza -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#Pizza"/>
        <rdfs:comment xml:lang="en">HighCaloriePizza that has a calorific value higher than or equal to 400</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#Hot -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#Hot">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#SpicinessValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#InterestingPizza -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#InterestingPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
                        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#Pizza"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#JalopenoPepperTopping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#JalopenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza/odontologies/pizza.owl#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#KiwiTooping -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#KiwiTooping">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#FruitsTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#LowCaloriePizza -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#LowCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza/odontologies/pizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#Pizza"/>
        <rdfs:comment xml:lang="en">HighCaloriePizza that has a calorific value less than  400</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza/odontologies/pizza.owl#MargheritaPizza -->

    <owl:Class rdf:about="http://www.pizza/odontologies/pizza.owl#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza/odontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza/odontologies/pizza.owl#hasTopping"/>
